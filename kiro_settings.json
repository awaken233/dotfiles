{
  // 跟随主题
  "window.autoDetectColorScheme": true,
  "editor.fontFamily": "\"JetBrains Mono\"",
  "editor.fontLigatures": true,
  "editor.suggestSelection": "first", 
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "update.enableWindowsBackgroundUpdates": false,
  "explorer.confirmDelete": false,
  "json.maxItemsComputed": 6000,
  "editor.bracketPairColorization.enabled": true,
  "telemetry.enableTelemetry": false,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.inlineSuggest.enabled": true,
  "window.enableMenuBarMnemonics": false,
  "editor.fontSize": 13.5,
  "redhat.telemetry.enabled": false,
  "editor.cursorSurroundingLines": 8,
  // leader key 设置为空格
  "vim.leader": "<space>",
  "vim.hlsearch":true,
  // 相对行号
  "vim.smartRelativeLine": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-e>": false,
    "<C-f>": false,
    "<C-t>": false,
  },
  "vim.replaceWithRegister": true,
  "vim.argumentObjectOpeningDelimiters":[ "(", "[", "<", "{" ],
  "vim.argumentObjectClosingDelimiters": [
    ")",
    "]",
    ">",
    "}"
  ],
  "vim.highlightedyank.enable": true,
  "vim.easymotion":true,
  "vim.normalModeKeyBindingsNonRecursive": [
    // 变量符号重命名
    { "before": [ "j" ], "after": [ "g", "j"] },
    { "before": [ "k" ], "after":["g", "k"] },
    { "before": [ "R" ], "commands": [ "editor.action.rename" ] },
    // 定义速览
    { "before": [ "K" ], "commands": [ "editor.action.showHover" ] },
    // 转到定义
    { "before": [ "g", "d" ], "commands": [ "editor.action.revealDefinition" ] },
    // 转到实现
    { "before": [ "g", "i" ], "commands": [ "editor.action.goToImplementation" ] },
    { "before": [ "g", "I" ], "after": ["g", "i"] },
    // 全局搜索文件名
    { "before": [ "<leader>", "f" ], "commands": [ "workbench.action.quickOpen" ] },
    { "before": [ "<leader>", "<space>" ], "commands": [ "workbench.action.quickOpen" ] },
    // 全局搜索符号
    { "before": [ "<leader>", "s" ,"s"], "commands": [ "workbench.action.showAllSymbols" ] },
    { "before": ["s"], "after": ["<leader>", "<leader>", "2", "s"] },
    // 打开文件浏览器
    { "before": [ "<leader>", "e" ], "commands": [ "workbench.view.explorer" ] },
    // 关闭当前标签页
    { "before": [ "<leader>", "x" ], "commands": [ "workbench.action.closeActiveEditor" ] },
    // 打开最近的项目
    { "before": [ "<leader>", "`" ], "commands": [ "workbench.action.openRecent", ], },
    { "before": [ "<Esc>"], "commands": [ ":nohl", "removeSecondaryCursors"]},
    { "before": [ "Y"], "after":["y", "$"] },
    { "before": ["+"], "after": ["<C-a>"] },
    { "before": ["-"], "after": ["<C-x>"] },
    { "before": ["g", "I"], "after": ["g", "i"] },
    { "before": ["t"], "after": ["F"] },
    { "before": [ "g", "f"], "commands": [ "workbench.action.gotoSymbol"], 
    "when": "!accessibilityHelpIsShown && !accessibleViewIsShown"},
    { "before": [ "<leader>", "e"], "commands": [ "workbench.view.explorer"] },
    { "before": [ "<leader>", "a"], "commands": [ "workbench.action.showCommands"] },
    { "before": [ "<leader>", "f"], "commands": [ "workbench.action.quickOpen"] },
    { "before": [ "g", "i"], "commands": [ "editor.action.goToImplementation"], 
      "when":"editorHasImplementationProvider && editorTextFocus" },
    { "before": [ "H"], "commands": [ "workbench.action.previousEditor"] },
    { "before": [ "L"], "commands": [ "workbench.action.nextEditor"] },
    { "before": [ "=", "="], "commands": [ "editor.action.formatDocument"], 
      "when":"editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor" },
    { "before": [ "[", "["], "commands": [ "gotoNextPreviousMember.previousMember"] },
    { "before": [ "]", "]"], "commands": [ "gotoNextPreviousMember.nextMember"] },
    { "before": ["g", "t"], "commands": ["editor.action.goToTypeDefinition"] },
    { "before": ["q"], "commands": ["editor.action.quickFix"] },
    { "before": ["<leader>", "g", "g"], "commands": ["git.checkout"] },
    { "before": ["<leader>", "r", "u"], "commands":["workbench.action.debug.start"]},
    { "before": ["<leader>", "r", "e"], "commands":["workbench.action.debug.restart"], "when": "inDebugMode"},
    { "before": ["<leader>", "d", "s"], "commands":["workbench.action.debug.stepOver"]},
    { "before": ["<leader>", "d", "o"], "commands":["workbench.action.debug.stepOut"]},
    { "before": ["<leader>", "d", "i"], "commands":["workbench.action.debug.stepInto"]},
    { "before": ["<leader>", "d", "b"], "commands":["editor.debug.action.toggleBreakpoint"]},
    { "before": ["<leader>", "d", "x"], "commands":["workbench.action.debug.stop"]},
    { "before": ["g", "a"], "commands": [ "gitlens.toggleFileBlame" ], 
    "when": "editorTextFocus && config.gitlens.keymap == 'alternate' && resource in 'gitlens:tabs:blameable'" },
    { "before": [ "<leader>", "h", "a" ], "commands": [ "workbench.action.maximizeEditorHideSidebar" ], },
    { "before": ["g", ";"], "commands": ["workbench.action.navigatePreviousInEditLocations"] },
    { "before": ["<leader>", "v", "c"], "commands":["git.viewChanges"]},
    { "before": ["<leader>", "q", "q"], "commands":["workbench.action.closeWindow"]},
    { "before": ["v", "v"], "after": ["V"] },
    { "before": ["<CR>"], "commands": [ "editor.action.smartSelect.expand" ] },
],
  "vim.visualModeKeyBindingsNonRecursive": [
    { "before": [ "<leader>", "<space>" ], "commands": [ "workbench.action.quickOpen" ] },
    { "before": [ "p", ], "after": [ "p", "g", "v", "y" ] },
    { "before": ["q"], "commands": ["editor.action.quickFix"] },
    { "before": [ "H"], "commands": [ "workbench.action.previousEditor"] },
    { "before": [ "L"], "commands": [ "workbench.action.nextEditor"] },
    { "before": ["<CR>"], "commands": [ "editor.action.smartSelect.expand" ] },
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
  ],"diffEditor.renderSideBySide": true,
  "diffEditor.experimental.showMoves": true,
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": true
  },
  // 代码标尺宽度
  "editor.rulers": [ 120 ],
  // mybatisx 说开启这个体验更好
  "editor.codeLens": true,
  "gitlens.graph.minimap.enabled": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  // 没有 staged 时提交所有更改
  // vscode 不会自动 git add, 开启这个选项比较方便
  "git.enableSmartCommit": true,
  // 建议启用智能提交(不再弹出智能提交提示框)
  "git.suggestSmartCommit": true,
  // 自动提交所有, 不要只提交 staged 的文件
  "git.smartCommitChanges": "all",
  "files.exclude": {
    "**/__pycache__": true,
    "*.iml": true,
    "target": true,
    ".idea": true,
    ".classpath": true,
    ".project": true,
    ".settings": true,
    ".factorypath": true
  },
  "search.exclude": {
    "**/.venv": true,
    "**/venv": true,
    ".obsidian": true
  },
  "java.compile.nullAnalysis.mode": "automatic",
  "spring.initializr.defaultJavaVersion": "8",
  "spring.initializr.defaultLanguage": "Java",
  "spring.initializr.defaultPackaging": "JAR",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.blame.avatars": false,
  "gitlens.blame.format": "${agoOrDateShort|10?} ${author|5-}",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
  "diffEditor.ignoreTrimWhitespace": true,
  "sonarlint.rules": {
    "java:S6813": {
      "level": "off"
    },
    "java:S3252": {
      "level": "off"
    }
  },
  "searchEverywhere.include": "**/*.{java,xml,py,md}",
  // java import order
  "java.completion.importOrder": [
    "",
    "javax",
    "java",
    "#"
  ],
  "foam.files.newNotePath": "currentDir",
  "security.promptForLocalFileProtocolHandling": false,
  "cursor.composer.collapsePaneInputBoxPills": true,
  "xml.server.preferBinary": true,
  "git.openRepositoryInParentFolders": "never",
  "gitlens.launchpad.indicator.enabled": false,
  "extensions.ignoreRecommendations": true,
  "terminal.integrated.copyOnSelection": true,
  "python.analysis.autoImportCompletions": true,
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "explorer.confirmPasteNative": false,
  "files.autoSaveDelay": 1000,
  "files.autoSave": "onFocusChange",
  "editor.foldingImportsByDefault": true,
  "workbench.editor.enablePreview": false,
  "cursor.cpp.disabledLanguages": [],
  "augment.chat.userGuidelines": "Always return to Simplified Chinese",
  "python.createEnvironment.trigger": "off",
  "diffEditor.diffAlgorithm": "advanced",
  "diffEditor.maxComputationTime": 0,
  "cursor.cpp.enablePartialAccepts": true,
  "Lingma.aI Chat.webToolsInAsk/AgentMode": "Auto-execute",
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.inlayHints.callArgumentNames": "partial",
  "git.blame.editorDecoration.enabled": false,
  "git.blame.statusBarItem.enabled": false,
  "githd.blameView.enabled": false,
  "cursorpyright.analysis.autoImportCompletions": true,
  "cursorpyright.analysis.inlayHints.callArgumentNames": "partial",
  "cursorpyright.analysis.inlayHints.functionReturnTypes": true,
  "cursorpyright.analysis.inlayHints.variableTypes": true,
  "cursorpyright.analysis.typeCheckingMode": "basic",
  "http.systemCertificates": true,
  "http.proxyStrictSSL": true,
  "cursor.enable_git_worktrees_setting": true,
  "cursor.agent_layout_browser_beta_setting": true,
  // 符号搜索支持搜索方法名
  "java.symbols.includeSourceMethodDeclarations": true,
  "java.jdt.ls.java.home": "/Library/Java/JavaVirtualMachines/zulu-21.jdk/Contents/Home",
  "git.showCursorWorktrees": true,
  "editor.accessibilitySupport": "off",
  "claudeCode.useTerminal": true,
  "claudeCode.preferredLocation": "panel",
  "editor.minimap.enabled": false,
  "kiroAgent.modelSelection": "claude-opus-4.5",
  "kiroAgent.agentAutonomy": "Autopilot",
  "telemetry.dataSharingAndPromptLogging.contentCollectionForServiceImprovement": false,
  "telemetry.editStats.enabled": false,
  "telemetry.feedback.enabled": false,
  "rest-client.enableTelemetry": false,
  "telemetry.dataSharingAndPromptLogging.usageAnalyticsAndPerformanceMetrics": false,
  "window.commandCenter": false,
  "workbench.activityBar.location": "top",
  "kiroAgent.enableTabAutocomplete": true,
  "kiroAgent.trustedCommands": [
    "ls *",
    "fd *",
    "rg *",
    "mvn *",
    "grep *",
    "sed *",
    "tail *",
    "head *",
    "mv *",
    "wc *",
    "cp *",
    "curl *",
    "find *",
    "mkdir *",
    "cd *",
    "nvim *",
    "awk *",
    "cat *",
    "tree *",
    "date *",
    "git log *",
    "pwd *",
    "git status *",
    "git --version *",
    "df *",
    "git diff *",
    "echo *",
    "cut *",
    "sort *",
    "uniq *",
    "netstat *",
    "ifconfig *",
    "dig *",
    "nslookup *",
    "npx *",
    "npm *",
    "ps *",
    "route *",
    "gh repo list *",
    "gh repo view *",
    "gh issue list *",
    "gh issue view *",
    "gh issue status *",
    "gh pr list *",
    "gh pr view *",
    "gh pr status *",
    "gh pr checks *",
    "gh pr diff *",
    "gh workflow list *",
    "gh release list *",
    "gh release view *",
    "gh search *",
    "gh gist list *",
    "gh gist view *",
    "git add *",
    "git commit *",
    "git --no-pager *",
    "gh --version *",
    "brew list *",
    "brew services *",
    "ping *",
    "shortcuts *",
    "git show *",
    "javac *",
    "java *",
    "gh api *",
    "git merge-base *",
    "jq *",
    "git tag -l *",
    "gh label list *",
    "gh run list *",
    "kubectl get *",
    "kubectl describe *",
    "which *",
    "du *",
    "stat *",
    "git mv *",
    "ghostty --version"
  ],
  "security.workspace.trust.untrustedFiles": "open",
  "diffEditor.hideUnchangedRegions.enabled": true,
  "workbench.preferredDarkColorTheme": "Monokai Dimmed",
  "workbench.preferredLightColorTheme": "Solarized Light",
  "kiroAgent.trustedTools": [
    "webFetch",
    "remote_web_search"
  ],
}